		{if}
			0 (S.L.door_45_nothahn) (S.L.doorTarget_45)
		{endif}
	{endif}

	(C.L.HST_Bremse_aktiv)
	{if}
		(L.L.door_0) 0 =
		(L.L.door_1) 0 = &&
