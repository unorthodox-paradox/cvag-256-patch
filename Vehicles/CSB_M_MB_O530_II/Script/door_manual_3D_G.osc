	(L.L.door_5) 0.75 > (S.L.PAX_Exit2_Open) (S.L.PAX_Exit3_Open)

'	Nothahn-Rebelüftung

	(L.L.door_01_nothahn_sw)
	{if}
		1 (S.L.door_01_nothahn)
	{endif}
	(L.L.door_01_nothahn)
	(L.L.door_01_nothahn_sw) ! &&
	{if}
		(L.L.door_0_soll) s0
		(L.L.doorDragAcc_0) s1
		1 s2
		1 s3
